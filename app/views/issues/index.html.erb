<p id="notice"><%= notice %></p>

<h1>Issues</h1>

<table>
  <thead>
    <tr>
      <th>Journal</th>
      <th>Volume</th>
      <th>Number</th>
      <th>Year</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @issues.each do |issue| %>
      <tr>
        <td><%= issue.journal %></td>
        <td><%= issue.volume %></td>
        <td><%= issue.number %></td>
        <td><%= issue.year %></td>
        <td><%= link_to 'Show', issue %></td>
      </tr>
    <% end %>
  </tbody>
</table>
